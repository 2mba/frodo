<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="404.0" prefWidth="604.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.GameView">
    <bottom>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
    </bottom>
   <center>
      <TableView fx:id="storeView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="storeColumnName" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="storeColumnPrice" prefWidth="75.0" text="Price" />
            <TableColumn fx:id="storeColumnType" prefWidth="75.0" text="Type" />
        </columns>
      </TableView>
   </center>
</BorderPane>
